<h1><a href="/artists/<%=@song.artist.slug%>"><%= @song.artist.name %></a></h1>
<h2><%= @song.name %></h2>
<h3>
    <% @song.genres.each do |genre| %>
        <a href="/genres/<%=genre.slug%>"><%= genre.name %></a>
    <% end %>
</h3>
<% if @success_message %>
  <div id="success" style="border: 2px solid green;">
  <%= @success_message %>
  </div>
<% end %>